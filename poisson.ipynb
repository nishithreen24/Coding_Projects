    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "import math\n",
    "lam = 5\n",
    "s = np.random.poisson(lam, 10000)\n",
    "count, k, ignored = plt.hist(s, 14, density=True)\n",
    "x = np.arange(k.min(), k.max(), 1)\n",
    "plt.plot(x, (np.exp(-lam) * (lam**k)) / math.factorial(k))\n",
    "plt.show()"
   ]
